{
  "presets": [
    ["env", {"modules": false}]
  ],
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "test": {
      "presets": ["env", "react"]
    },
    "rollup": {
      "presets": [["env", {"modules": false}], "react"],
      "plugins": [
        "transform-object-rest-spread",
        "external-helpers",
        "transform-class-properties",
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }]
      ]
    }
  }
}
